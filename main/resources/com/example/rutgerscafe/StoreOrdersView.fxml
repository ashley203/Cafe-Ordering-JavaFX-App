<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.rutgerscafe.StoreOrdersController">
   <children>
      <Label alignment="CENTER" layoutX="225.0" layoutY="30.0" prefHeight="60.0" prefWidth="150.0" text="Store Orders" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <ComboBox fx:id="comboStoreOrdersQuantity" layoutX="47.0" layoutY="90.0" onAction="#showOrders" prefWidth="150.0" />
      <TextField fx:id="totalText" layoutX="410.0" layoutY="91.0" />
      <Label layoutX="375.0" layoutY="96.0" text="Total" />
      <Button layoutX="210.0" layoutY="351.0" mnemonicParsing="false" onAction="#cancelOrder" text="Cancel Order" />
      <Button layoutX="344.0" layoutY="351.0" mnemonicParsing="false" onAction="#exportOrders" text="Export Orders" />
      <ListView fx:id="storeOrdersList" layoutX="47.0" layoutY="137.0" prefHeight="200.0" prefWidth="523.0" />
   </children>
</AnchorPane>
